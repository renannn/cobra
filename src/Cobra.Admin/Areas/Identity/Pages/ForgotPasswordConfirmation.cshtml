@page
@model Cobra.Admin.Areas.Identity.Pages.ForgotPasswordConfirmationModel
@{
}


<div class="row">
	<div class="col-sm-12">
		<!-- Authentication card start -->

		<form class="md-float-material form-material"
			  asp-route-returnurl="@ViewData["ReturnUrl"]"
			  method="post"
			  autocomplete="off">
			<div class="text-center">
				<img src="~/assets/images/logo.png" alt="logo.png">
			</div>
			<div class="auth-box card">
				<div class="card-block">
					<div class="row m-b-20">
						<div class="col-md-12">
							<h3 class="text-center">Recuperar senha!</h3>
						</div>
					</div>

					Por favor, verifique sua caixa de email para recuperar sua senha.
					<partial name="_PickupFolder" />

				</div>
			</div>
		</form>
		<!-- end of form -->
	</div>
	<!-- end of col-sm-12 -->
</div>